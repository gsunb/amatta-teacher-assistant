{
  "version": 2,
  "buildCommand": "",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.1.5"
    },
    "api/health.js": {
      "runtime": "@vercel/node@3.1.5"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}